{
    "fields": [
        {
            "name": "apps",
            "type": "array",
            "instance": "new plcipc.PLCApplication(${address}, ${symbols})",
            "lookup": {
                "fetch": "rest.get('/automation/api/v1.0/plc/app?type=browse')",
                "transform": "{ id: arguments[0], address: arguments[0], symbols: (() => { const ret = rest.get(`/automation/api/v1.0/plc/app/${arguments[0]}/sym?type=browse`); return ret.map((s) => ({ id: ret, vtype: 'TODO' })) })() }"
            },
            "sync": {
                "symbols": {
                    "fetch": "(() => { const ret = rest.get(`/automation/api/v1.0/plc/app/${arguments[0]}/sym?type=browse`); return ret.map((s) => rest.get(`/automation/api/v1.0/plc/app/${arguments[0]}/sym/${s}?type=browse`).map((v) => arguments[0] + '/' + v); })()",
                    "transform": "{ id: arguments[0], vtype: 'TODO' }"
                }
            },
            "fields": [
                {
                    "name": "id",
                    "type": "string"
                },
                {
                    "name": "address",
                    "type": "string"
                },
                {
                    "name": "symbols",
                    "type": "array",
                    "fields": [
                        {
                            "name": "id",
                            "type": "string"
                        },
                        {
                            "name": "vtype",
                            "type": "string"
                        }
                    ]
                }
            ]
        }
    ],
    "default": {
        "apps": [
            {
                "id": "plc1",
                "address": "plc1",
                "symbols": [
                    {
                        "id": "testinger",
                        "vtype": "UINT"
                    }
                ]
            }
        ]
    }
}
